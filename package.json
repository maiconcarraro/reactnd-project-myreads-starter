{
  "dependencies": {
    "group-array": "^0.3.3",
    "prettier": "^1.14.0",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-autosuggest": "^9.4.0",
    "react-dom": "^16.3.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/index.js",
      "!src/api/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "lcov",
      "text"
    ]
  }
}
